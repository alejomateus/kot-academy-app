    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "libphonenumber-js": "^1.11.5",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "sonar-scanner": "^3.1.0",
    "typescript": "~5.4.0"
  },
  "description": "An Ionic project"
}
